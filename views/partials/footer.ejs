<script>
  function openNav() {
    document.getElementById("secSidebar").style.width = "290px";
    document.getElementById("topnav").style.paddingLeft = "290px";
    document.getElementById("content").style.marginLeft = "290px";
    document.getElementById("openbtn").onclick = function () {
      closeNav();
    };
  }

  function closeNav() {
    document.getElementById("secSidebar").style.width = "70px";
    document.getElementById("topnav").style.paddingLeft = "70px";
    document.getElementById("content").style.marginLeft = "70px";
    document.getElementById("openbtn").onclick = function () {
      openNav();
    };
  }
</script>
